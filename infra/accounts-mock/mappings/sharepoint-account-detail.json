{
  "request": {
    "method": "POST",
    "url": "/sharepoint-services/api/v1/account/getAccountDetail"
  },
  "response": {
    "status": 200,
    "headers": { "Content-Type": "application/json" },
    "transformers": ["response-template"],
    "jsonBody": {
      "cutomerDetail": {
        "customerName": "Client {{#jsonPath request.body '$.customerCode'}}{{.}}{{/jsonPath}}",
        "accountList": [
          { "agencyCode": "AG001", "accountNumber": "{{randomValue length=10 type='NUMERIC'}}", "suffix": "01", "currencyCode": 840 },
          { "agencyCode": "AG002", "accountNumber": "{{randomValue length=10 type='NUMERIC'}}", "suffix": "00", "currencyCode": 181 }
        ]
      }
    }
  }
}
