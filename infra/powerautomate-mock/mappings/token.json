{
  "request": {
    "method": "POST",
    "urlPath": "/oauth2/token"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["response-template"],
    "jsonBody": {
      "access_token": "{{randomValue length=32 type='ALPHANUMERIC'}}",
      "token_type": "Bearer",
      "expires_in": 3600
    }
  }
}
